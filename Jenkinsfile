pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building the image from development state'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing the image from development state'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying the image'
            }
        }
    }
}
